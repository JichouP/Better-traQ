{
  "name": "better-traq",
  "version": "1.0.0",
  "description": "An Extension that Makes traQ Useful",
  "main": "index.js",
  "scripts": {
    "watch:traQ": "env-cmd -e traQ --use-shell gulp watch",
    "watch:ex-traQ": "env-cmd -e ex-traQ --use-shell gulp watch",
    "build:both": "npm run build:traQ && npm run build:ex-traQ",
    "build:traQ": "env-cmd -e traQ --use-shell gulp build",
    "build:ex-traQ": "env-cmd -e ex-traQ --use-shell gulp build",
    "test": "jest --color",
    "test:coverage": "npm run test -- --collectCoverage",
    "test:badge": "npm run test:coverage && jest-coverage-badges",
    "test:badge:add": "npm run test:badge && git add coverage",
    "tsc:babelConfig": "tsc babel.config.ts --skipLibCheck --newLine LF && prettier --write babel.config.js",
    "lint": "eslint src",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JichouP/Better-traQ.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JichouP/Better-traQ/issues"
  },
  "homepage": "https://github.com/JichouP/Better-traQ#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "archiver": "^5.3.0",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webextension-polyfill-ts": "^0.25.0",
    "zod": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/archiver": "^5.1.0",
    "@types/chrome": "^0.0.141",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.0",
    "@types/webpack-node-externals": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy-webpack-plugin": "^9.0.0",
    "esbuild": "^0.12.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "gulp": "^4.0.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "=4.2.4",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.{md,html,css,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov"
    ]
  }
}
